<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>CrossPlatformLibrary.Maps</id>
    <version>1.0.3-alpha</version>
    <title>CrossPlatformLibrary.Maps</title>
    <authors>Thomas Galliker</authors>
    <licenseUrl>http://opensource.org/licenses/Apache-2.0</licenseUrl>
    <projectUrl>https://nuget.org/packages/CrossPlatformLibrary.Maps</projectUrl>
    <iconUrl>https://nuget.org/Content/Images/packageDefaultIcon-50x50.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <summary>CrossPlatformLibrary.Maps is a plug-in for map related concerns.</summary>
    <description>CrossPlatformLibrary.Maps is a plug-in for map related concerns. It enables cross-platform map functionality.</description>
    <releaseNotes />
    <copyright>Copyright 2015</copyright>
    <language>en-US</language>
    <dependencies>

      <!--CommonServiceLocator-->
      <group targetFramework="MonoTouch10">
        <dependency id="CommonServiceLocator" version="1.3" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="CommonServiceLocator" version="1.3" />
      </group>
      <group targetFramework="MonoAndroid10">
        <dependency id="CommonServiceLocator" version="1.3" />
      </group>
      <group targetFramework="wp8">
        <dependency id="CommonServiceLocator" version="1.3" />
      </group>
      <group targetFramework="portable-net45">
        <dependency id="CommonServiceLocator" version="1.3" />
      </group>

      <!--CrossPlatformLibrary-->
      <group targetFramework="MonoTouch10">
        <dependency id="CrossPlatformLibrary" />
      </group>
      <group targetFramework="Xamarin.iOS10">
        <dependency id="CrossPlatformLibrary" />
      </group>
      <group targetFramework="MonoAndroid10">
        <dependency id="CrossPlatformLibrary" />
      </group>
      <group targetFramework="wp8">
        <dependency id="CrossPlatformLibrary" />
      </group>
      <group targetFramework="portable-net45">
        <dependency id="CrossPlatformLibrary" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <!--PCL-->
    <file src="..\CrossPlatformLibrary.Maps\bin\Release\CrossPlatformLibrary.Maps.dll" target="lib\portable-net45+wp8+MonoAndroid10+MonoTouch10+Xamarin.iOS10\CrossPlatformLibrary.Maps.dll" />

    <!--Xamarin.Android-->
    <file src="..\CrossPlatformLibrary.Maps\bin\Release\CrossPlatformLibrary.Maps.dll" target="lib\MonoAndroid10\CrossPlatformLibrary.Maps.dll" />
    <file src="..\CrossPlatformLibrary.Maps.Android\bin\Release\CrossPlatformLibrary.Maps.Platform.dll" target="lib\MonoAndroid10\CrossPlatformLibrary.Maps.Platform.dll" />

    <!--Xamarin.iOS Classic API-->
    <file src="..\CrossPlatformLibrary.Maps\bin\Release\CrossPlatformLibrary.Maps.dll" target="lib\MonoTouch10\CrossPlatformLibrary.Maps.dll" />
    <file src="..\CrossPlatformLibrary.Maps.iOS\bin\iPhone\Release\CrossPlatformLibrary.Maps.Platform.dll" target="lib\MonoTouch10\CrossPlatformLibrary.Maps.Platform.dll" />

    <!--Xamarin.iOS Unified API-->
    <file src="..\CrossPlatformLibrary.Maps\bin\Release\CrossPlatformLibrary.Maps.dll" target="lib\Xamarin.iOS10\CrossPlatformLibrary.Maps.dll" />
    <file src="..\CrossPlatformLibrary.Maps.iOSUnified\bin\iPhone\Release\CrossPlatformLibrary.Maps.Platform.dll" target="lib\Xamarin.iOS10\CrossPlatformLibrary.Maps.Platform.dll" />

    <!--Windows Phone 8-->
    <file src="..\CrossPlatformLibrary.Maps\bin\Release\CrossPlatformLibrary.Maps.dll" target="lib\wp8\CrossPlatformLibrary.Maps.dll" />
    <file src="..\CrossPlatformLibrary.Maps.WindowsPhone8\Bin\Release\CrossPlatformLibrary.Maps.Platform.dll" target="lib\wp8\CrossPlatformLibrary.Maps.Platform.dll" />

    <file src="tools\init.ps1" target="tools\init.ps1" />
    <file src="tools\install.ps1" target="tools\install.ps1" />
    <file src="tools\uninstall.ps1" target="tools\uninstall.ps1" />
  </files>
